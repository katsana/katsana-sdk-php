{
    "name": "katsana/katsana-sdk-php",
    "description": "KATSANA SDK for PHP",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\Sdk\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5",
        "laravie/codex": "~0.4.2 || ~0.5",
        "php-http/multipart-stream-builder": "^0.1.1"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "~4.8 || ~5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
